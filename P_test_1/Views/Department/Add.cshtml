@model Department
@{
    ViewData["Title"] = "Add";
}

<h1>Add</h1>

<form method="post" action="/Department/saveAdd">
    <div>
        <label>Name</label>
        <input name="Name" id="Name" value="@Model?.Name" type="text" class="form form-control" />
    </div>
    <div>
        <label>Manger Name</label>
        <input name="MangerName" id="MangerName" value="@Model?.MangerName" type="text" class="form form-control" />
    </div>
    <input type="submit" value="Create" class="btn btn-primary" />
</form>

<a href="/department/index">Back to List</a>